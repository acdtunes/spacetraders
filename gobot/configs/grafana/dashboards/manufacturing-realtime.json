          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    p.product_good,\n    p.status,\n    p.sell_market,\n    p.created_at,\n    COALESCE(completed.cnt, 0)::float / NULLIF(total.cnt, 0)::float as progress\nFROM manufacturing_pipelines p\nLEFT JOIN LATERAL (\n    SELECT COUNT(*) as cnt FROM manufacturing_tasks t\n    WHERE t.pipeline_id = p.id AND t.status = 'COMPLETED'\n) completed ON true\nLEFT JOIN LATERAL (\n    SELECT COUNT(*) as cnt FROM manufacturing_tasks t\n    WHERE t.pipeline_id = p.id\n) total ON true\nWHERE p.status IN ('PLANNING', 'EXECUTING')\nORDER BY p.created_at DESC",

      "targets": [
        {
          "datasource": { "type": "grafana-postgresql-datasource", "uid": "postgresql" },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    COALESCE(p.product_good, 'N/A') as pipeline,\n    t.task_type,\n    t.good,\n    t.priority as base_priority,\n    t.priority + LEAST(EXTRACT(EPOCH FROM (NOW() - t.ready_at))/30, 100)::int as effective_priority,\n    t.status,\n    ROUND(EXTRACT(EPOCH FROM (NOW() - t.ready_at))/60, 1) as waiting_min,\n    COALESCE(t.factory_symbol, t.source_market) as source,\n    t.target_market as destination\nFROM manufacturing_tasks t\nLEFT JOIN manufacturing_pipelines p ON t.pipeline_id = p.id\nWHERE t.status IN ('PENDING', 'READY')\nORDER BY\n    t.priority + LEAST(EXTRACT(EPOCH FROM (NOW() - t.ready_at))/30, 100)::int DESC,\n    t.ready_at ASC\nLIMIT 20",
          "refId": "A"
        }
      ],
      "title": "Task Queue with Priority",
      "type": "table"
